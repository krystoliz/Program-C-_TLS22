#include <iostream>
using namespace std;
int main() {
    double x, y;
    double inchConst, footConst, height_in_cm;
    inchConst = 2.54;
    footConst = 30.48;
    
    cout << "feet = ";
    cin >> x;
    cout << "inch = ";
    cin >> y;
    
    height_in_cm = x * footConst + y * inchConst;
    cout << height_in_cm << " cm" << endl;
    
    return 0;
}
